{
    "form": {
        "properties": {
            "mch_options": {
                "title": "MHC Options",
                "type": "object",
                "properties": {
                    "alleles": {
                        "type": "string",
                        "title": "Alleles (Manual Entry)",
                        "description": "Used only if the alleles CSV is not provided below.",
                        "default": "A*01:01;A*02:01;B*07:02;B*24:02;C*03:01;C*04:01"
                    },
                    "alleles_csv": {
                        "title": "Alleles (CSV)",
                        "file": "**/spreadsheet_csv/**/spreadsheet.csv",
                        "pathType": "references",
                        "type": "string"
                    },
                    "mhc_class": {
                        "type": "string",
                        "title": "MHC Class",
                        "default": "I",
                        "enum": ["I", "II"]
                    }
                }
            },
            "analysis_options": {
                "title": "Analysis Options",
                "type": "object",
                "properties": {
                    "filter_self": {
                        "default": true,
                        "description": "Filter against human proteome",
                        "title": "Filter Self",
                        "type": "boolean",
                        "value": false
                    },
                    "max_peptide_length": {
                        "default": 11,
                        "description": "",
                        "title": "Maximum Peptide Length",
                        "type": "integer"
                    },
                    "min_peptide_length": {
                        "default": 8,
                        "description": "",
                        "title": "Minimum Peptide Length",
                        "type": "integer"
                    },
                    "max_peptide_length_class2": {
                        "default": 16,
                        "description": "",
                        "title": "Maximum Peptide Length (MHC Class II)",
                        "type": "integer"
                    },
                    "min_peptide_length_class2": {
                        "default": 15,
                        "description": "",
                        "title": "Minimum Peptide Length (MHC Class II)",
                        "type": "integer"
                    }
                }
            },
            "reference_options": {
                "title": "Reference Options",
                "type": "object",
                "properties": {
                    "proteome": {
                        "default": "",
                        "description": "Reference proteome file used for self-filtering",
                        "file": "**/genome_fasta/**/genome.fasta",
                        "pathType": "references",
                        "title": "Proteome Reference",
                        "type": "string"
                    }
                }
            },
            "analysis_tools": {
                "title": "Analysis Tools",
                "type": "object",
                "properties": {
                    "use_mhcflurry": {
                        "title": "Use mhcflurry",
                        "default": true,
                        "type": "boolean"
                    },
                    "use_syfpeithi": {
                        "title": "Use syfpeithi",
                        "default": true,
                        "type": "boolean"
                    }
                }
            }
        }
    },
    "ui": {}
}