{
  "form": {
    "type": "object",
    "title": "",
    "properties": {
      "genome": {
        "type": "object",
        "title": "Genome Information",
        "description": "Provide details on the organism which was sequenced",
        "required": [
          "fasta",
          "genus_species"
        ],
        "properties": {
          "fasta": {
            "type": "string",
            "title": "Genome",
            "file": "**/*.f*",
            "pathType": "dataset"
          },
          "genus_species": {
            "type": "string",
            "title": "Organism name",
            "description": "Genus and species (e.g. 'Escherichia coli')"
          },
          "strain": {
            "type": "string",
            "title": "Strain",
            "description": "(optional) Strain of the sequenced organism"
          }
        }
      },
      "contact": {
        "type": "object",
        "title": "Author / Contact Information",
        "description": "PGAP requires contact information to prepare genome assemblies for submission to NCBI RefSeq",
        "required": [
          "last_name",
          "first_name",
          "email",
          "organization",
          "department",
          "phone",
          "street",
          "city",
          "state",
          "postal_code",
          "country"
        ],
